<%= button_to "Return to Discover Page", "/users/#{@user.id}/discover", method: :get %>
<div id="movie-results">
  <% @results.each do |movie| %>
    <%= link_to "#{movie.title}", "/users/#{@user.id}/movies/#{movie.db_id}", method: :get %>
    <p>Rating: <%= movie.vote_average %></p>
  <% end %>
</div>
